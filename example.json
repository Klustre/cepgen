{
    "version": "6.0",
    "bundle": {
        "name": "My Bundle",
        "id": "com.mycompany",
        "version": "1.0.0"
    },
    "hosts": {
        "AEFT": "16.0"
    },
    "extensions": [
        {
            "menu": "My Panel",
            "type": "Panel",
            "version": "1.0.0",
            "main": "./index.html",
            "script": "./script.jsx",
            "id": "com.mycompany.panel",
            "autovisible": true,
            "debug": {
                "AEFT": 3001
            },
            "geometry": {
                "size": {
                    "width": 200,
                    "height": 100
                },
                "min": {
                    "width": 400,
                    "height": 200
                },
                "max": {
                    "width": 800,
                    "height": 400
                }
            },
            "icons": {
                "Normal": "./icons/icon.png",
                "Disabled": "./icons/icon.png",
                "Rollover": "./icons/icon.png",
                "DarkNormal": "./icons/icon.png",
                "DarkRollOver": "./icons/icon.png"
            },
            "events": [
                "com.lifecycle.event"
            ],
            "params": [
                "--enable-nodejs"
            ]
        }
    ]
}
